<div class='d-flex justify-content-between align-items-end'>
    <a href='/dashboard' class='text-decoration-none badge bg-success border-0'>Dashboard</a>
    <form action='/logout' method='post'>
        <button type='submit' class='badge bg-danger border-0'>Logout</button>
    </form>
</div>

<br>

<h3 class='text-capitalize small text-white'>{{student.firstname}} {{student.lastname}} | <span class='text-uppercase'>{{student.course}}</span></h3>

<br>

{{!-- {{firstYearfirstTrime subjects}} --}}
<div class='row mb-5 text-white' id='subjectDiv'>

    <h5>1st Year 1st Trimester</h5>
    {{{firstYearfirstTrime subjects student}}}

    <h5 class='mt-3'>1st Year 2nd Trimester</h5>
    {{{firstYearsecondTrime subjects student}}}

    <h5 class='mt-3'>1st Year 3rd Trimester</h5>
    {{{firstYearthirdTrime subjects student}}}

    <hr class='mt-4'>

    <h5>2nd Year 1st Trimester</h5>
    {{{secondYearfirstTrime subjects student}}}

    <h5 class='mt-3'>2nd Year 2nd Trimester</h5>
    {{{secondYearsecondTrime subjects student}}}

    <h5 class='mt-3'>2nd Year 3rd Trimester</h5>
    {{{secondYearthirdTrime subjects student}}}

    <hr class='mt-4'>

    <h5>3rd Year 1st Trimester</h5>
    {{{thirdYearfirstTrime subjects student}}}

    <h5 class='mt-3'>3rd Year 2nd Trimester</h5>

    {{{thirdYearsecondTrime subjects student}}}

    <h5 class='mt-3'>3rd Year 3rd Trimester</h5>
    {{{thirdYearthirdTrime subjects student}}}

    <hr class='mt-4'>

    <h5>4th Year 1st Trimester</h5>
    {{{fourthYearfirstTrime subjects student}}}

</div>